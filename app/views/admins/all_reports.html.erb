<div class="container">
  <div class="row">
    <div class="col-12">
      <%= link_to 'Home', admin_path %>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="col-12">
      <h2 style="text-decoration:underline;">All Reports</h2>
    </div>
  </div>
  <% @reports.each do |report| %>
    <div class="row">
      <div class="col-12">
        <%= link_to report.title, edit_report_path(report.id) %>
      </div>
    </div>
  <% end %>
</div>