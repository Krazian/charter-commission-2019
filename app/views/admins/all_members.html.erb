<div class="container">
  <div class="row">
    <div class="col-12">
      <%= link_to 'Home', admin_path %>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="col-12">
      <h2 style="text-decoration:underline;">All Commission Members</h2>
    </div>
  </div>
  <% @members.each do |member| %>
    <div class="row">
      <div class="col-12">
        <%= link_to member.name, edit_member_path(member.id) %>
      </div>
    </div>
  <% end %>
</div>