<div class="container">
  <div class="row">
    <div class="col-12">
      <%= link_to 'Home', admin_path %>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="col-12">
      <h2 style="text-decoration:underline;">All Staff Members</h2>
    </div>
  </div>
  <% @staff_members.each do |staff| %>
    <div class="row">
      <div class="col-12">
        <%= link_to staff.name, edit_staff_path(staff.id) %>
      </div>
    </div>
  <% end %>
</div>